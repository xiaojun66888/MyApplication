<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.app5.ShareMenuActivity">

    <TextView
        android:id="@+id/umeng_menu_bottom"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_weight="1"
        android:background="@color/umeng_white"
        android:gravity="center"
        android:text="页面底部菜单-1"
        android:textColor="@color/umeng_black" />

    <TextView
        android:id="@+id/umeng_menu_center"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_weight="1"
        android:background="@color/umeng_white"
        android:gravity="center"
        android:text="页面中部菜单-1"
        android:textColor="@color/umeng_black" />
</LinearLayout>
